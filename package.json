{
  "name": "portfolio",
  "private": true,
  "homepage": "https://kemalborabayraktar.com",
  "description": "Kemal Bora Bayraktar's Portfolio",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "deploy": "gatsby build && gh-pages -d public",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint:js:check": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:js:fix": "eslint --ext .js,.jsx,.ts,.tsx . --fix",
    "lint:css:check": "stylelint \"**/*.css\" \"**/*.scss\" \"**/*.js\" \"**/*.tsx\"",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "review": "npm run lint:js:check && npm run prettier:check && npm run lint:css:check",
    "format": "npm run lint:js:fix && npm run prettier:fix"
  },
  "dependencies": {
    "@fontsource/comforter": "^4.5.8",
    "@fontsource/inter": "^4.5.15",
    "@fontsource/montserrat": "^4.5.14",
    "@react-icons/all-files": "^4.1.0",
    "babel-plugin-styled-components": "^2.0.7",
    "gatsby": "^5.2.0",
    "gatsby-plugin-image": "^3.2.0",
    "gatsby-plugin-manifest": "^5.7.0",
    "gatsby-plugin-sharp": "^5.2.0",
    "gatsby-plugin-sitemap": "^6.7.0",
    "gatsby-plugin-styled-components": "^3.10.0",
    "gatsby-source-filesystem": "^5.2.0",
    "gatsby-transformer-sharp": "^5.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-typical": "^0.1.3",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "eslint": "^8.29.0",
    "gh-pages": "^5.0.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.1",
    "stylelint": "^14.16.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0"
  }
}
